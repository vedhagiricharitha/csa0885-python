s1 = str(input("ENter word 1: "))
s2 = str(input("Enter word 2: "))

s1 = s1.lower()
s2 = s2.lower()

if(len(s1) == len(s2)):

    sorted_s1 = sorted(s1)
    sorted_s2 = sorted(s2)

    if(sorted_s1 == sorted_s2):
        print(" Both are anagram.")
    else:
        print(" Both are not anagram.")

else:
    print("Both are not anagram.")
